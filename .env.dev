# FantasyF1 Development Environment Variables
# This file is used for the Docker Dev Container

# Security
SECRET_KEY=dev_secret_key_change_in_production_use_strong_random_string
JWT_SECRET_KEY=dev_jwt_secret_key_change_in_production
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# Database
POSTGRES_USER=fantasyf1_dev
POSTGRES_PASSWORD=dev_password_123
POSTGRES_DB=fantasyf1_dev

# Redis
REDIS_PASSWORD=dev_redis_123

# MQTT
MQTT_BROKER_HOST=mosquitto
MQTT_BROKER_PORT=1883
MQTT_USERNAME=fantasyf1_dev
MQTT_PASSWORD=dev_mqtt_123

# Application
ENVIRONMENT=development
DEBUG=true
CORS_ORIGINS=http://localhost:8002,http://localhost:3000

# Ports (Only backend and flower are exposed to host)
BACKEND_PORT=8002
FLOWER_PORT=5555

# Docker Images
BACKEND_IMAGE=fantasyf1-be-dev:latest
